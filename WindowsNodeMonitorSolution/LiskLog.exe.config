<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <appSettings>

    <add key="enviroment" value="PROD MONITOR"/>
    <add key="enablerebuild" value="1"/>
    <add key="port" value="7000"/>
    <add key="timerticksms" value="120000"/>
    <!--<add key="peers" value="13.69.159.242;40.68.34.176;52.165.40.188;13.82.31.30;13.91.61.2"/>-->
    <add key="peers" value="testnet.lisk.io"/>
    <add key="blockdiftorebuild" value="65"/>
    <add key="intervalToRebuildMinuts" value="75"/>
    <add key="emailto" value=""/>

    <!--commands replace # by &&-->

    <!--prod mode-->
    <add key="rebuild" value="cd $home # cd lisk # bash lisk.sh rebuild"/>
    <add key="restart" value="cd $home # cd lisk  # bash lisk.sh restart # bash lisk.sh reload"/>
    <add key="start" value="cd $home # cd lisk # bash lisk.sh start"/>

  </appSettings>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
</configuration>